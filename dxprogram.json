{
  "name": "gatk_recalibrate",
  "title": "GATK Recalibration and Realignment",
  "summary": "Performs quality recalibration recommended in GATK best practices in variant detection",  
  "dxapi": "1.03rc",
  "version": "0.1",
  "inputSpec": [
    {"name": "mappings", "class": "gtable"},
    {"name": "known_dbsnp", "class": "array:gtable"},
    {"name": "known_indels", "class": "array:gtable"},
    {"name": "discard_unmapped", "class":"boolean", "default":true},
    {"name": "discard_duplicates", "class":"boolean", "default": true},
    {"name": "covariate_read_threshold", "class":"int", "default":50000000},
    {"name": "mark_duplicates_chunks", "class":"int", "default":5}
  ],
  "outputSpec": [
  {"name": "ok", "class": "boolean"}
  ],
  "runSpec": 
    {"file": "runRecalibrate.py", "interpreter": "python2.7_old", "execDepends": [{"name": "openjdk-6-jre"}]}
}
